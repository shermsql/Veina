{
  "schemaVersion": 1,
  "id": "veina",
  "version": "${version}",
  "name": "Veina",
  "description": "Break One Ore, And Watch The Whole Vein Collapse. Fast, Simple, Satisfying.",
  "authors": ["shermsql (Batuhan Kurkut)"],
  "contact": {
    "github": "https://github.com/shermsql",
    "email": "Kurkut.Batuhan@outlook.com"
  },
  "license": "MIT",
  "icon": "assets/veina/icon.png",
  "environment": "server",
  "entrypoints": {
    "fabric-datagen": [
      "org.shermsql.veina.client.VeinaDataGenerator"
    ],
    "main": [
      "org.shermsql.veina.Veina"
    ]
  },
  "mixins": [
    "veina.mixins.json",
    {
      "config": "veina.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
